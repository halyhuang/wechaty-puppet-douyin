# Python 路径
python=C:\Python311\python.exe

# Visual Studio 版本
msvs_version=2022

# Electron 版本
electron_version=22.3.0

# npm 镜像
registry=https://registry.npmmirror.com/
ELECTRON_MIRROR=https://registry.npmmirror.com/-/binary/electron/
ELECTRON_BUILDER_BINARIES_MIRROR=https://registry.npmmirror.com/-/binary/electron-builder-binaries/

# node-gyp 配置
target=22.3.0
runtime=electron
disturl=https://electronjs.org/headers/
target_arch=x64
target_libc=unknown
runtime=electron
build_from_source=true

# 强制从源码编译
npm_config_build_from_source=true
build_from_source=true
prefer_offline=false
prefer_online=true
node_pre_gyp_build_from_source=true
grpc_build_from_source=true
grpc_node_pre_gyp_build_from_source=true

# 环境变量
ELECTRON_MIRROR=https://registry.npmmirror.com/-/binary/electron/
ELECTRON_BUILDER_BINARIES_MIRROR=https://registry.npmmirror.com/-/binary/electron-builder-binaries/
npm_config_target=22.3.0
npm_config_runtime=electron
npm_config_disturl=https://electronjs.org/headers/
npm_config_target_arch=x64
npm_config_target_libc=unknown
npm_config_runtime=electron
npm_config_build_from_source=true

# Node.js 配置
node_mirror=https://registry.npmmirror.com/-/binary/node/
node_headers_mirror=https://registry.npmmirror.com/-/binary/node/

# 包管理器设置
prefer-npm=true
save-exact=true

# 日志设置
loglevel=info

# 缓存配置
prefer-offline=false
prefer-online=true

# 环境变量配置
# DOUYIN_TOKEN=${DOUYIN_TOKEN}
# DOUYIN_APP_ID=${DOUYIN_APP_ID}
# DOUYIN_APP_SECRET=${DOUYIN_APP_SECRET}

# Node 环境
NODE_ENV=development

# TypeScript 配置
typescript=true
ts-node=true

# Git 配置
git-checks=true

# 编译配置
msbuild_path=C:\Program Files\Microsoft Visual Studio\2022\Community\MSBuild\Current\Bin\MSBuild.exe
msbuild_arch=x64
msbuild_configuration=Release

# Visual Studio 2022 工具链配置
GYP_MSVS_VERSION=2022
windows_build_tools_path=C:\Program Files (x86)\Microsoft Visual Studio\2022\BuildTools
vs_path=C:\Program Files\Microsoft Visual Studio\2022\Community
vs_toolchain=14.37
vs_buildtools_version=17.7

# 编译架构配置
target_arch=x64
target_libc=unknown

# Electron 特定配置
electron_mirror=https://registry.npmmirror.com/-/binary/electron/
electron_builder_binaries_mirror=https://registry.npmmirror.com/-/binary/electron-builder-binaries/
electron_custom_dir=22.3.0

# Node.js 版本配置
node_version=16.20.2

# jq 配置
JQ_BINARY_PATH=node_modules/node-jq/bin/jq.exe


